{
  "name": "ethereum-ticker-ui",
  "version": "0.0.1",
  "description": "UI for ethereum-ticker",
  "scripts": {
    "start": "npm run prod",
    "dev": "npm run build:dev && npm run start-dev-server",
    "prod": "npm run build:prod && npm run start-server",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --colors",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --colors",
    "linter": "eslint src -f html --ext .jsx --ext .js -o lint-report.html",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "start-dev-server": "cross-env NODE_ENV=development node server.js",
    "start-server": "cross-env NODE_ENV=production node server.js"
  },
  "author": "Mike Chabot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikechabot/ethereum-ticker.git"
  },
  "bugs": {
    "url": "https://github.com/mikechabot/ethereum-ticker/issues"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.23.0",
    "bulma": "^0.6.2",
    "chart.js": "^2.3.0",
    "cross-env": "^5.0.5",
    "express": "^4.16.2",
    "glamor": "^2.20.40",
    "glamorous": "^4.11.4",
    "jquery": "^3.3.1",
    "lodash": "^4.17.4",
    "maybe-baby": "^1.0.20",
    "moment": "^2.19.1",
    "prop-types": "^15.6.0",
    "react": "^15.5.0-rc.2",
    "react-chartjs-2": "^2.7.0",
    "react-dom": "^15.5.0-rc.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-entity": "^5.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "del": "^3.0.0",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^11.3.0",
    "json-loader": "^0.5.7",
    "mocha": "^4.0.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.7",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-tabify": "0.0.24",
    "redux-mock-store": "^1.3.0",
    "sass-loader": "^6.0.6",
    "sinon": "^4.0.1",
    "style-loader": "^0.19.0",
    "svg-react-loader": "^0.4.4",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.1"
  }
}
